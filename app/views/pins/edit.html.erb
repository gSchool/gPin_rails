   <h4>Edit Pin</h4>

    <%= form_for @pin do |f| %>
      <%= f.label "Image URL" %>
      <%= f.text_field :image %><br />
      <%= f.label "Short Description" %>
      <%= f.text_field :comment %><br />
      <%= f.label "Page URL" %>
      <%= f.text_field :url %><br />
      <%= f.submit "New Doug Pin" %>
    <% end %>