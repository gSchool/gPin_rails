<h1>Pins</h1>
<% @pins.each do |pin|%>
  <div>
    <% if pin.image%>
      <a href="<%=pin.url%>">
        <img src="<%= pin.image%>" >
      </a>
    <%else%>
    <a href="<%=pin.url%>">
      <img src="http://a5.mzstatic.com/us/r30/Purple3/v4/9d/94/ee/9d94eee0-9d4c-05dd-e7a4-2ca851753695/icon175x175.png">
    </a>
    <%end%>
    <p><%= pin.name %></p>
    <a href="/pins/<%= pin.id %>">View Pin</a>
    </div>
<% end %>